<?xml version="1.0"?>
<robot name="my_rb1_robot">
    <!-- LINK DEFINITIONS -->
    <!-- Base link -->
    <link name="base_link">
        <!-- Base link visual properties -->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.3"/>
            </geometry>
            <material name="Base">
                <color rgba="0.1373 0.1373 0.1373 1"/>
            </material>
        </visual>
        <!-- Base link collision properties -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.3"/>
            </geometry>
        </collision>
        <!-- Base link inertial properties -->
        <inertial>
            <mass value="22" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.50875" ixy="0" ixz="0" iyy="0.50875" iyz="0" izz="0.11458" />
        </inertial>
    </link>
    <!-- Base footprint -->
    <link name="base_footprint">
    </link>
    <!-- Front laser -->
    <link name="front_laser">
        <!-- Front laser visual properties -->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
            <material name="Laser">
                <color rgba="0 1 0 1"/>
            </material>
        </visual>
        <!-- Front laser collision properties -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
        </collision>
        <!-- Front laser inertial properties -->
        <inertial>
            <mass value="0.25" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001" />
        </inertial>
    </link>
    <!-- Left wheel --> 
    <link name="left_wheel">
        <!-- Left wheel visual properties -->
        <visual>
            <origin rpy="1.5708 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.05" length="0.05"/>
            </geometry>
            <material name="Wheel">
                <color rgba="0.8902 0 0.1804 1"/>
            </material>
        </visual>
        <!-- Left wheel collision properties -->
        <collision>
            <origin rpy="1.5708 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.05" length="0.05"/>
            </geometry>
        </collision>
        <!-- Left wheel inertial properties -->
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00083" ixy="0" ixz="0" iyy="0.00083" iyz="0" izz="0.00021" />
        </inertial>
    </link>
    <!-- Right wheel --> 
    <link name="right_wheel">
        <!-- Right wheel visual properties -->
        <visual>
            <origin rpy="1.5708 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.05" length="0.05"/>
            </geometry>
            <material name="Wheel">
                <color rgba="0.8902 0 0.1804 1"/>
            </material>
        </visual>
        <!-- Right wheel collision properties -->
        <collision>
            <origin rpy="1.5708 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder radius="0.05" length="0.05"/>
            </geometry>
        </collision>
        <!-- Right wheel inertial properties -->
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00083" ixy="0" ixz="0" iyy="0.00083" iyz="0" izz="0.00021" />
        </inertial>
    </link>
    <!-- Front caster --> 
    <link name="front_caster">
        <!-- Front caster visual properties -->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="Wheel">
                <color rgba="0.8902 0 0.1804 1"/>
            </material>
        </visual>
        <!-- Front caster collision properties -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>
        <!-- Front caster inertial properties -->
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000125" ixy="0" ixz="0" iyy="0.000125" iyz="0" izz="0.000125" />
        </inertial>
    </link>
    <!-- Back caster --> 
    <link name="back_caster">
        <!-- Back caster visual properties -->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="Wheel">
                <color rgba="0.8902 0 0.1804 1"/>
            </material>
        </visual>
        <!-- Back caster collision properties -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>
        <!-- Back caster inertial properties -->
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000125" ixy="0" ixz="0" iyy="0.000125" iyz="0" izz="0.000125" />
        </inertial>
    </link>
    <!-- JOINT DEFINITIONS -->
    <!-- Base : Base footprint -->
    <joint name="base_link_base_footprint_joint" type="fixed">
        <!-- Avoid Gazebo conflicts
        <parent link="base_footprint"/>
        <child link="base_link"/> -->
        <parent link="base_link"/>
        <child link="base_footprint"/> 
        <origin xyz="0 0 -0.2" rpy="0 0 0"/> 
    </joint>
    <!-- Front laser : Base -->
    <joint name="front_laser_base_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_laser"/>
        <origin xyz="0.25 0.0 0.075" rpy="3.1416 0 0"/>
    </joint>
    <!-- Left wheel : Base -->
    <joint name="left_wheel_base_link_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis xyz="0 1 0" />
    </joint>
    <!-- Right wheel : Base -->
    <joint name="right_wheel_base_link_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis xyz="0 1 0" />
    </joint>
    <!-- Front caster : Base -->
    <joint name="front_caster_base_link_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 -0.175" />
        <child link="front_caster" />
        <parent link="base_link" />
    </joint>
    <!-- Back caster : Base -->
    <joint name="back_caster_base_link_joint" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 -0.175" />
        <child link="back_caster" />
        <parent link="base_link" />
    </joint>
    <!-- GAZEBO CONFIGURATION -->
    <!-- Base link color -->
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
    <!-- Base footprint friction -->
    <gazebo reference="base_footprint">
        <mu1>0</mu1>
        <mu2>0</mu2>
    </gazebo>
    <!-- Left wheel color and friction --> 
    <gazebo reference="left_wheel">
        <material>Gazebo/Red</material>
        <mu1>1</mu1>
        <mu2>1</mu2>
    </gazebo>
    <!-- Right wheel color and friction --> 
    <gazebo reference="right_wheel">
        <material>Gazebo/Red</material>
        <mu1>1</mu1>
        <mu2>1</mu2>
    </gazebo>
    <!-- Front caster color and friction --> 
    <gazebo reference="front_caster">
        <material>Gazebo/Red</material>
        <mu1>0</mu1>
        <mu2>0</mu2>
    </gazebo>
    <!-- Back caster color and friction --> 
    <gazebo reference="back_caster">
        <material>Gazebo/Red</material>
        <mu1>0</mu1>
        <mu2>0</mu2>
    </gazebo>
    <!-- Front caster : Base -->
    <gazebo reference="front_caster_base_link_joint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <!-- Back caster : Base -->
    <gazebo reference="back_caster_base_link_joint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <!-- Front laser : Base --> 
    <gazebo reference="front_laser_base_joint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <!-- Base : Base footprint --> 
    <gazebo reference="base_link_base_footprint_joint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <!-- Front laser color --> 
    <gazebo reference="front_laser">
        <material>Gazebo/GreenGlow</material>
    </gazebo>
    <!-- Differential drive plugin -->
    <gazebo>
        <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
            <alwaysOn>true</alwaysOn>
            <updateRate>20</updateRate>
            <leftJoint>left_wheel_base_link_joint</leftJoint>
            <rightJoint>right_wheel_base_link_joint</rightJoint>
            <wheelSeparation>0.4</wheelSeparation>
            <wheelDiameter>0.1</wheelDiameter>
            <torque>10</torque>
            <commandTopic>cmd_vel</commandTopic>
            <odometryTopic>odom</odometryTopic>
            <odometryFrame>odom</odometryFrame>
            <robotBaseFrame>base_link</robotBaseFrame>
        </plugin>
    </gazebo>
    <!-- Laser scan sensor -->
    <gazebo reference="front_laser">
        <sensor type="ray" name="head_hokuyo_sensor">
            <pose>0 0 0 0 0 0</pose>
            <visualize>true</visualize>
            <update_rate>20</update_rate>
            <ray>
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-1.570796</min_angle>
                        <max_angle>1.570796</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.02</min>
                    <max>10.0</max>
                    <resolution>0.01</resolution>
                </range>
                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
            <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
                <topicName>/scan</topicName>
                <frameName>sensor_laser</frameName>
            </plugin>
        </sensor>
    </gazebo>
</robot>
